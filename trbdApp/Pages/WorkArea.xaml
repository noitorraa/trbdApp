<Page x:Class="trbdApp.Pages.WorkArea"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:trbdApp.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="WorkArea">

    <Grid>
        <TextBlock Text="Вы вошли с систему!" VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="26" Margin="50"></TextBlock>
        <Button x:Name="btnPrint" Content="Созадать файл со списком сотрудников" FontSize="26" Width="500" Height="50" Click="btnPrint_Click"></Button>
    </Grid>
</Page>
